{
  "name": "zora-dev-server",
  "version": "0.0.4",
  "description": "test runner for browser programs, using zora testing library",
  "main": "dist/bundles/zora-dev-server",
  "bin": {
    "zds": "dist/bundles/bin.js"
  },
  "scripts": {
    "test:public": "",
    "test:unit": "pta test/unit/{handlers,lib}/*.js test/unit/*.js",
    "test:coverage": "c8 npm run test:unit",
    "build": "rollup -c rollup/build.js && cp src/bin.js dist/bundles/bin.js && cp -R src/public dist/bundles"
  },
  "keywords": [
    "test",
    "testing",
    "browser",
    "test-harness",
    "tap",
    "zora"
  ],
  "author": "Laurent RENARD",
  "license": "MIT",
  "dependencies": {
    "arg": "^4.1.1",
    "debug": "^4.1.1",
    "fast-glob": "^3.1.0",
    "http-errors": "^1.7.3",
    "mime-types": "^2.1.25",
    "resolve": "^1.12.0",
    "zora": "^3.1.4"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "nodemon": "^1.19.4",
    "pta": "^0.1.0",
    "rollup": "^1.27.0",
    "smart-table-core": "^3.0.7",
    "supertest": "^4.0.2"
  }
}
